{
  "manifest_version": 3,
  "name":"snappage",
  "description": "summarizes text of current website",
  "version": "0.0.1",
  "action": {
    "default_popup": "index.html",
    "default_title": "Open"
  },
  "permissions": ["scripting"],
  "host_permissions": [
    "http://*/",
    "https://*/"
  ]
}